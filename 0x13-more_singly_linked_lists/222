#include "lists.h"
/**
 * print_listint - prints all the elements of a listint_t list.
 * @h: head pointing to first node
 * Return: the number of nodes
 */
size_t print_listint(const listint_t *h)
{
	const listint_t *h, *ptr;
	size_t count = 0;

	if (h == NULL)
		return (0);

	while (ptr != NULL)
	{
		printf("%d\n", ptr->n);
		count++;
		h = h->next;
	}
	return (count);
}

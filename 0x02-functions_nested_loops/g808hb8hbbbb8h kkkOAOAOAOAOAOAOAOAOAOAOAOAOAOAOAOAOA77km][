       [1mint isalnum_l(int [4m[22mc[24m[1m, locale_t [4m[22mlocale[24m[1m);[0m
       [1mint isalpha_l(int [4m[22mc[24m[1m, locale_t [4m[22mlocale[24m[1m);[0m
